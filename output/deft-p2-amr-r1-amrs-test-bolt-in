(m2 / multi-sentence
      :snt1 (e / experiment-01
            :ARG0 (m / military :wiki "Unit_731"
                  :name (n / name :op1 "Unit" :op2 731)
                  :mod (c3 / country :wiki "Japan"
                        :name (n2 / name :op1 "Japan")))
            :ARG1 (a2 / and
                  :op1 (p / person
                        :ARG0-of (l / live-01
                              :mod (c5 / country :wiki "China"
                                    :name (n4 / name :op1 "China"))))
                  :op2 (p3 / person
                        :ARG0-of (l3 / live-01)
                        :mod (c6 / country :wiki "Korea"
                              :name (n5 / name :op1 "Korea")))
                  :op3 (p4 / person
                        :ARG0-of (l4 / live-01)
                        :mod (c7 / country
                              :mod (o / other))))
            :ARG2 (m3 / method
                  :ARG1-of (b / brutal-02
                        :degree (e2 / extreme)))
            :mod (b2 / body
                  :mod (h / human))
            :manner (c2 / cruel)
            :location (c4 / country-region :wiki "Northeast_China"
                  :name (n3 / name :op1 "Northeast")))
      :snt2 (a3 / add-02
            :ARG0 (i / incident :wiki "Nanking_Massacre"
                  :name (n6 / name :op1 "Nanjing" :op2 "Massacre"))
            :ARG1 (l2 / layer
                  :consist-of (b3 / blood)
                  :ARG1-of (f / fade-01 :polarity -
                        :time (e3 / ever)
                        :direction (a5 / away)))
            :ARG2 (c / city
                  :ARG0-of (h2 / have-org-role-91
                        :ARG2 (c8 / capital))
                  :mod (a4 / ancient)))
      :snt3 (a6 / and
            :op1 (b4 / bare-01
                  :ARG0 (b5 / battle-01
                        :mod (b6 / big)
                        :mod (b7 / bloody)
                        :location (a7 / and
                              :op1 (c9 / city :wiki "Hengyang"
                                    :name (n7 / name :op1 "Hengyang"))
                              :op2 (c10 / city :wiki "Changsha"
                                    :name (n8 / name :op1 "Changsha"))))
                  :ARG1 (b8 / bone
                        :part-of (p2 / person
                              :ARG1-of (d / die-01)))
                  :quant (o2 / over
                        :op1 (d2 / distance-quantity :quant 1000
                              :unit (k2 / kilometer))))
            :op2 (k / kill-off-02
                  :ARG0 b5
                  :ARG1 (e4 / everything))))

