                              :ARG2 (o / out))))
            :condition (l / lose-03
                  :ARG0 w3))
      :snt5 (l3 / let-01 :mode imperative
            :ARG0 (y2 / you)
            :ARG1 (a3 / and
                  :op1 (h / have-03
                        :ARG0 (c7 / country :wiki "China"
                              :name (n4 / name :op1 "China"))
                        :ARG1 (b2 / blood
                              :mod (n5 / new)))
