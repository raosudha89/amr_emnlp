(y / youth
      :ARG1-of (t / tender-02)
      :time (s / still)
      :domain (p / person
            :ARG0-of (h / have-rel-role-91
                  :ARG2 (b / brother
                        :mod (y2 / young
                              :degree (m / most))))))

