                  :ARG0 (i2 / i)))
      :path (p / page
            :mod (t2 / that)
            :ARG1-of (y / yellow-01)
            :mod (h / history))
      :manner (r2 / resemble-01
            :ARG2 (s / see-01
                  :ARG0 i2
                  :ARG1 (a / and
                        :op1 (p3 / person :wiki "Kangxi_Emperor" :name (n / name :op1 "Kangxi")
                              :ARG0-of (c / contemplate-01
                                    :ARG1 (t8 / thing
                                          :manner-of (a5 / achieve-01
                                                :ARG0 p3
                                                :ARG1 (u / unify-01
                                                      :ARG0 p3
                                                      :ARG1 (t5 / territory))))
