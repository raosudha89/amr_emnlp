(s / suffer-01
      :ARG0 (c / country :wiki "China" :name (n / name :op1 "China")
            :mod (e / especially))
      :ARG1 (r / ravage-01
            :ARG0 (c2 / country :wiki "Japan" :name (n2 / name :op1 "Japan"))
            :ARG1 c)
      :duration (s4 / several
            :op1 (t / temporal-quantity :quant 1
                  :unit (d3 / decade))
            :ARG1-of (l / long-03)))

