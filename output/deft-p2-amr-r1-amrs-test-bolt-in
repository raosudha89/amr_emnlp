# ::edge	name	op1	"Ms."	0.0.2.0.0.1	0.0.2.0.0.1.0	
# ::edge	name	op2	"Fan"	0.0.2.0.0.1	0.0.2.0.0.1.1	
# ::edge	near-02	ARG2	person	0.0.2.0	0.0.2.0.0	
# ::edge	obligate-01	ARG1	person	0.1.1	0.1.1.0	
# ::edge	obligate-01	ARG2	drink-01	0.1.1	0.1.1.1	
# ::edge	obligate-01	ARG2-of	have-condition-91	0.1.1	0.1.1.2	
# ::edge	or	op1	force-01	0.1.1.2.0	0.1.1.2.0.0	
# ::edge	or	op2	frame-02	0.1.1.2.0	0.1.1.2.0.1	
# ::edge	person	ARG0-of	act-01	0.1.1.0	0.1.1.0.0	
# ::edge	person	name	name	0.0.2.0.0	0.0.2.0.0.1	
# ::edge	person	wiki	-	0.0.2.0.0	0.0.2.0.0.0	
# ::edge	wolf	ARG0-of	have-03	0.0.2	0.0.2.1	
# ::edge	wolf	ARG1-of	near-02	0.0.2	0.0.2.0	
(m / multi-sentence
      :snt1 (l / look-01 :mode imperative
            :ARG0 (y2 / you)
            :ARG1 (w / wolf
                  :ARG1-of (n / near-02
                        :ARG2 (p / person :wiki - :name (n2 / name :op1 "Ms." :op2 "Fan")))
                  :ARG0-of (h2 / have-03
                        :ARG1 (g / glasses))))
      :snt2 (k / know-01
            :ARG0 (y / you)
            :ARG1 (o / obligate-01
                  :ARG1 (p2 / person
                        :ARG0-of (a2 / act-01))
                  :ARG2 (d / drink-01
                        :ARG0 p2
                        :accompanier (h / he))
                  :ARG2-of (h3 / have-condition-91
                        :ARG1 (o2 / or
                              :op1 (f / force-01 :polarity -
                                    :ARG1 p2
                                    :ARG2 (o3 / out))
                              :op2 (f2 / frame-02 :polarity -
                                    :ARG1 p2
                                    :mod (e / even)))))))

# ::node	0	let-01	0-1
# ::node	0.0	imperative	
# ::node	0.1	you	
# ::node	0.2	drink-01	3-4
# ::node	0.2.0	horse	2-3
# ::node	0.2.1	ocean	6-8
# ::node	0.2.1.0	"Pacific_Ocean"	
# ::node	0.2.1.1	name	6-8
# ::node	0.2.1.1.0	"Pacific"	6-8
# ::node	0.2.1.1.1	"Ocean"	6-8
# ::root	0	let-01
# ::edge	drink-01	ARG0	horse	0.2	0.2.0	
# ::edge	drink-01	ARG2	ocean	0.2	0.2.1	
# ::edge	let-01	ARG0	you	0	0.1	
# ::edge	let-01	ARG1	drink-01	0	0.2	
# ::edge	let-01	mode	imperative	0	0.0	
# ::edge	name	op1	"Pacific"	0.2.1.1	0.2.1.1.0	
# ::edge	name	op2	"Ocean"	0.2.1.1	0.2.1.1.1	
# ::edge	ocean	name	name	0.2.1	0.2.1.1	
# ::edge	ocean	wiki	"Pacific_Ocean"	0.2.1	0.2.1.0	
(l / let-01 :mode imperative
      :ARG0 (y / you)
      :ARG1 (d / drink-01
            :ARG0 (h / horse)
            :ARG2 (o / ocean :wiki "Pacific_Ocean"
                  :name (n / name :op1 "Pacific" :op2 "Ocean"))))

