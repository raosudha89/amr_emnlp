# ::node	0	and	3-4
# ::node	0.0	brandish-01	0-1
# ::node	0.0.0	we	
# ::node	0.0.1	weapon	2-3
# ::node	0.1	cross-02	4-5
# ::node	0.1.0	east	7-8
# ::root	0	and
# ::edge	and	op1	brandish-01	0	0.0	
# ::edge	and	op2	cross-02	0	0.1	
# ::edge	brandish-01	ARG0	we	0.0	0.0.0	
# ::edge	brandish-01	ARG1	weapon	0.0	0.0.1	
# ::edge	cross-02	ARG0	we	0.1	0.0.0	
# ::edge	cross-02	direction	east	0.1	0.1.0	
# ::edge	weapon	poss	we	0.0.1	0.0.0	
(a / and
      :op1 (b / brandish-01
            :ARG0 (w2 / we)
            :ARG1 (w / weapon
                  :poss w2))
      :op2 (c / cross-02
            :ARG0 w2
            :direction (e / east)))

