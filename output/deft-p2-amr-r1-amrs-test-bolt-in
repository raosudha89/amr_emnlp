      :snt1 (l4 / let-01 :mode imperative
            :ARG0 (y / you)
            :ARG1 (k / know-01
                  :ARG0 (t2 / they)
                  :ARG1 (p / panacea :polarity -
                        :domain (c / country :wiki "United_States"
                              :name (n / name :op1 "US")))))
      :snt2 (p2 / possible-01
            :ARG1 (i / increase-01
                  :ARG0 w
                  :ARG1 (i2 / influence-01
                        :ARG0 (c2 / country :wiki "China"
                              :name (n2 / name :op1 "China"))))
            :condition (w / win-01
                  :ARG0 (w2 / we)))
      :snt3 (a / and
            :op2 (i3 / improve-01
                  :ARG1 (c3 / cohesion
                        :mod (c4 / citizen))))
      :snt4 (p3 / possible-01
            :ARG1 (a2 / and
                  :op1 (c5 / clean-out-03
                        :ARG0 (w3 / we)
