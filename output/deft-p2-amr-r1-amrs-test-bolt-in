      :snt1 (p / person
            :ARG1-of (s / score-01
                  :ARG3 (t2 / thing :wiki "Imperial_examination" :name (n / name :op1 "Imperial" :op2 "Examination"))
                  :degree (t / top))
            :location (w / walk :quant 360
                  :quant (a / all)
                  :mod (l / life)))
      :snt2 (a2 / and
            :op1 (e / effort)
            :op2 (e2 / endeavor-01)
            :location (b / behind
                  :op1 (p2 / person
                        :ARG1-of (s2 / score-01
                              :ARG3 (t4 / thing :wiki "Imperial_examination" :name (n2 / name :op1 "Imperial" :op2 "Examination"))
                              :degree (t6 / top))))
            :degree (s3 / so))
      :snt3 (h / humiliate-01
            :ARG0 (t5 / this)
            :ARG1 (w3 / word
                  :mod (v / vermicelli
                        :mod (b2 / bean)))))

