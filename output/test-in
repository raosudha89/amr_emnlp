      :ARG0 (p2 / person :wiki "Dominique_de_Villepin"
            :name (n3 / name :op1 "Dominique" :op2 "de" :op3 "Villepin")
            :ARG0-of (h / have-org-role-91
                  :ARG1 (c2 / country :wiki "France"
                        :name (n / name :op1 "France"))
                  :ARG2 (m / minister
                        :mod (f / foreign))))
      :ARG1 (c / consider-02
            :ARG0 (g / government-organization
                  :ARG0-of (g2 / govern-01
                        :ARG1 c2))
            :ARG1 (c3 / crack-down-06
                  :ARG1 (o / organization :wiki "People's_Mujahedin_of_Iran"
                        :name (n2 / name :op1 "People's" :op2 "Mujahedeen")
