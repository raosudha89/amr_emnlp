      :ARG0 (a2 / and
            :op1 (c / country :wiki "China" :name (n / name :op1 "China"))
            :op2 (c2 / country :wiki "Kyrgyzstan" :name (n2 / name :op1 "Kyrgyzstan")))
      :ARG1 (a3 / and
            :op1 (c4 / conform-01
                  :ARG1 (a4 / and
                        :op1 (p / purpose
                              :poss (o / organization :wiki "Shanghai_Cooperation_Organisation" :name (n3 / name :op1 "Shanghai" :op2 "Cooperation" :op3 "Organization")))
                        :op2 (p2 / principle
                              :poss o))
                  :ARG2 (r / reality))
            :op2 (c5 / conducive
                  :domain (a5 / and
                        :op1 (t / trend
                              :mod (d / develop-02))
                        :op2 (a6 / activity-06
                              :ARG0 o))
                  :purpose (a7 / and
                        :op1 (p3 / peace
                              :mod (r2 / region))
                        :op2 (s / stability
                              :mod r2))))
      :mod (f / further)
      :medium (c3 / communique))

