                        :mod (w / weapon
                              :mod (n2 / nucleus))
                        :ARG0-of (d / declare-02 :polarity -))))
      :op2 (s3 / say-01
            :ARG0 c
            :ARG1 (r / recommend-01
                  :ARG1 (a2 / allow-01
                        :ARG1 (e / engage-01
                              :ARG1 (s4 / state)
                              :ARG2 (t / trade-01
                                    :mod n2))
                        :condition (a3 / apply-02
                              :ARG0 s4
                              :ARG1 (a4 / and
                                    :op1 (p / protect-01
                                          :mod (p2 / physical)
                                          :mod (s5 / stringent))
                                    :op2 (m / measure-02
                                          :ARG1 (c3 / control-01))
                                    :op3 (m2 / measure-02
                                          :ARG1 (a5 / accountancy)))
                              :ARG2 (a6 / and
                                    :op1 (w2 / weapon
                                          :mod n2
                                          :quant (a7 / all)
                                          :location (t2 / territory
                                                :poss s4))
                                    :op2 (f / facility
                                          :mod n2
                                          :quant a7
                                          :location t2)
                                    :op3 (m3 / material
                                          :mod (s6 / source)
                                          :quant a7
                                          :location t2)
                                    :op4 (m4 / material
                                          :mod n2
                                          :ARG1-of (s7 / special-02)
                                          :quant a7
                                          :location t2)))))))

