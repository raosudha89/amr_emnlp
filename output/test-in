# ::edge	multi-sentence	snt2	extravagant	0	0.1	
# ::edge	person	ARG0-of	have-org-role-91	0.1.1	0.1.1.0	
(m / multi-sentence
      :snt1 (c / carefree
            :domain (l / life
                  :poss (c2 / citizen
                        :poss (w / we))
                  :quant (a / all))
            :time (c3 / current)
            :degree (t / too))
      :snt2 (e / extravagant
            :degree (t2 / too)
            :domain (p / person
                  :ARG0-of (h / have-org-role-91
                        :ARG2 (o2 / official)))))

