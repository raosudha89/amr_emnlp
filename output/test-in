      :op1 (r / report-01
            :ARG0 (p5 / person :wiki "Vladimir_Popovkin"
                  :name (n / name :op1 "Vladimir" :op2 "Popovkin")
                  :ARG0-of (h / have-org-role-91
                        :ARG1 (m / military :wiki "Russian_Space_Forces"
                              :name (n2 / name :op1 "Russian" :op2 "Space" :op3 "Forces"))
                        :ARG2 (c / commander))
                  :ARG0-of (h2 / have-org-role-91
                        :ARG2 (l / lieutenant-general)))
            :ARG2 (a2 / and
                  :op1 (p2 / person :wiki "Jacques_Chirac"
                        :name (n3 / name :op1 "Chirac"))
                  :op2 (p3 / person :wiki "Vladimir_Putin"
                        :name (n4 / name :op1 "Putin"))))
      :op2 (s / say-01
            :ARG0 p5
            :ARG1 (i / include-91
                  :ARG1 (e / expand-01
                        :ARG1 (c2 / cooperate-01
                              :ARG0 (c3 / country :wiki "Russia"
                                    :name (n5 / name :op1 "Russia"))
                              :ARG1 (c4 / country :wiki "France"
                                    :name (n6 / name :op1 "France"))
                              :ARG2 (s2 / space)))
                  :ARG2 (p4 / priority
                        :poss c3))))

