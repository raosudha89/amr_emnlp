      :ARG1 (t2 / trade-01
            :ARG0 (c / country :wiki "Brazil" :name (n / name :op1 "Brazil"))
            :ARG2 (c2 / country :wiki "South_Africa" :name (n2 / name :op1 "South" :op2 "Africa")))
      :ARG2 (a / and
            :op1 (m / monetary-quantity :quant 696000000
                  :unit (d / dollar
                        :mod (c3 / country :wiki "United_States" :name (n3 / name :op1 "United" :op2 "States")))
                  :time (d2 / date-entity :year 2002))
            :op2 (m3 / monetary-quantity :quant 296000000
                  :mod (m2 / more)
                  :purpose (b / balance-01
                        :ARG1 (t3 / trade-01)
                        :ARG1-of (g / go-10
                              :ARG2 c))
                  :unit d)))

