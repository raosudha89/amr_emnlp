# ::node	0.1.1.1.1.0	"Nepal"	13-14
# ::node	0.1.2	reimpose-01	
# ::node	0.1.3	yet	17-18
# ::root	0	and
# ::edge	and	op1	expire-01	0	0.0	
# ::edge	and	op2	decide-01	0	0.1	
# ::edge	country	name	name	0.1.1.1	0.1.1.1.1	
# ::edge	country	wiki	"Nepal"	0.1.1.1	0.1.1.1.0	
# ::edge	date-entity	day	28	0.0.1	0.0.1.2	
# ::edge	date-entity	dayperiod	evening	0.0.1	0.0.1.3	
# ::edge	date-entity	month	8	0.0.1	0.0.1.1	
# ::edge	date-entity	year	2002	0.0.1	0.0.1.0	
# ::edge	decide-01	ARG0	government-organization	0.1	0.1.1	
# ::edge	decide-01	ARG1	reimpose-01	0.1	0.1.2	
# ::edge	decide-01	polarity	-	0.1	0.1.0	
# ::edge	decide-01	time	yet	0.1	0.1.3	
# ::edge	expire-01	ARG1	state	0.0	0.0.0	
# ::edge	expire-01	time	date-entity	0.0	0.0.1	
# ::edge	government-organization	ARG0-of	govern-01	0.1.1	0.1.1.0	
# ::edge	government-organization	mod	country	0.1.1	0.1.1.1	
# ::edge	name	op1	"Nepal"	0.1.1.1.1	0.1.1.1.1.0	
# ::edge	reimpose-01	ARG0	government-organization	0.1.2	0.1.1	
# ::edge	reimpose-01	ARG1	state	0.1.2	0.0.0	
# ::edge	state	mod	emergency	0.0.0	0.0.0.0	
(a / and
      :op1 (e / expire-01
            :ARG1 (s / state
                  :mod (e2 / emergency))
            :time (d2 / date-entity :year 2002 :month 8 :day 28
                  :dayperiod (e3 / evening)))
      :op2 (d / decide-01 :polarity -
            :ARG0 (g / government-organization
                  :ARG0-of (g2 / govern-01)
                  :mod (c / country :wiki "Nepal"
                        :name (n / name :op1 "Nepal")))
            :ARG1 (r / reimpose-01
                  :ARG0 g
                  :ARG1 s)
            :time (y / yet)))

