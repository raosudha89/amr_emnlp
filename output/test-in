      :op1 (f / found-01
            :ARG1 (f2 / facility
                  :mod (m / military)
                  :time (e / era
                        :mod (c / country :wiki "Soviet_Union"
                              :name (n / name :op1 "Soviet" :op2 "Union")))
                  :mod (c2 / close))
            :time (d2 / date-entity :year 2005 :month 7))
      :op2 (c3 / collect-01
            :ARG0 f2
            :ARG1 i)
      :op3 (a2 / analyze-01
            :ARG0 f2
            :ARG1 (i / information
                  :ARG1-of (g / gather-01
                        :source (s / satellite
                              :mod (s2 / spy-01)
                              :mod (m2 / military)
                              :mod (c4 / country :wiki "Russia"
                                    :name (n2 / name :op1 "Russia"))))
                  :quant (a3 / all))))

