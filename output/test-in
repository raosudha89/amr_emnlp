(t4 / tell-01
      :ARG0 (t3 / they)
      :ARG1 (g / get-05
            :ARG1 y2
            :ARG2 (o / on))
      :ARG2 (y2 / you)
      :mod (j / just)
      :ARG1-of (c / cause-01
            :ARG0 (l / lazy
                  :domain t3
                  :degree (h / hell)))
      :condition (t / take-01 :mode imperative
            :ARG0 (y / you)
            :ARG1 (n / note
                  :quant (m2 / monetary-quantity :quant 20
                        :unit (p / pound)))
            :location (b / bus)))

