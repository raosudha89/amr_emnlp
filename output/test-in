(p / period
      :time-of (a / and
            :op1 (a2 / add-02 :polarity -
                  :ARG0 (c / country :wiki "Iran" :name (n / name :op1 "Iran"))
                  :ARG1 (c2 / centrifuge))
            :op2 (r / refrain-01
                  :ARG0 (w2 / world-region :wiki "Western_world" :name (n2 / name :op1 "West"))
                  :ARG1 (p2 / promote-02
                        :ARG0 w2
                        :ARG1 (r2 / round-05
                              :ARG1 (s / sanction-02
                                    :ARG1 c
                                    :mod (e / economy))
                              :ord (o2 / ordinal-entity :value 4))
                        :ARG2 (o / organization :wiki "United_Nations_Security_Council" :name (n3 / name :op1 "UN" :op2 "Security" :op3 "Council")))))
      :time (p3 / prenegotiate-01)
      :mod (f / freeze-for-freeze)
      :duration (t / temporal-quantity :quant 6
            :unit (w / week)))

