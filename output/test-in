(b / become-01
      :ARG1 (p2 / person :wiki "Jacques_Chirac"
            :name (n2 / name :op1 "Jacques" :op2 "Chirac")
            :ARG0-of (h / have-org-role-91
                  :ARG1 (c / country :wiki "France"
                        :name (n3 / name :op1 "France"))
                  :ARG2 (p3 / president))
            :ARG0-of (v / visit-01))
      :ARG2 (p / person
            :ARG0-of (l / lead-02
                  :ARG1 (w / world-region :wiki "Western_world"
                        :name (n / name :op1 "West")))
            :ord (o / ordinal-entity :value 1)
            :ARG0-of (v2 / visit-01
                  :ARG1 (c2 / center :wiki "Titov_Main_Test_and_Space_Systems_Control_Centre"
                        :name (n4 / name :op1 "Titov")
                        :mod (s / space)
                        :mod (c4 / country :wiki "Russia"
                              :name (n5 / name :op1 "Russia"))
                        :mod (t2 / top-secret)
                        :mod (t / test-01)
                        :mod (c3 / control-01)))
            :time (d2 / date-entity :year 2004 :month 4 :day 3)))

