      :ARG0 (s / study
            :ARG1-of (p / publish-01
                  :ARG0 (g / government-organization :wiki "Defense_Intelligence_Agency"
                        :name (n / name :op1 "Defense" :op2 "Intelligence" :op3 "Agency")
                        :mod (c / country :wiki "United_States"
                              :name (n2 / name :op1 "US"))))
            :time (d4 / date-entity :year 2003))
      :ARG1 (o / originate-01
            :ARG1 (w / weapon
                  :ARG1-of (i / include-91
                        :ARG2 (w2 / weapon
                              :ARG1-of (s2 / seize-01
                                    :ARG2 (g2 / guerrilla
                                          :mod (c2 / country :wiki "Colombia"
                                                :name (n3 / name :op1 "Colombia")))
                                    :time (d3 / date-interval
                                          :op1 (d / date-entity :year 1998)
                                          :op2 (d2 / date-entity :year 2001)))
                              :ARG2-of (m2 / mark-02
                                    :ARG0 (m3 / military
                                          :mod (f / foreign))))
                        :ARG3 (m / more-than :op1 "1/2")))
            :ARG2 (c3 / country :wiki "Venezuela"
                  :name (n4 / name :op1 "Venezuela"))))

