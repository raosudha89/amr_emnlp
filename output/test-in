      :ARG0 (r / repository
            :mod (l / large)
            :consist-of (a / and
                  :op1 (a2 / anthrax)
                  :op2 (p / plague)
                  :op3 (b / bacterium
                        :mod (o / other))
                  :ARG0-of (k / kill-01))
            :ARG1-of (s / store-01
                  :ARG2 (c / complex
                        :mod (l3 / laboratory)
                        :mod (c2 / country :wiki "Russia" :name (n / name :op1 "Russia"))
                        :mod (s2 / security
                              :ARG1-of (h / high-02))
                        :location (r2 / relative-position
