      :ARG0 (p / person :wiki -
            :name (n / name :op1 "Carlos" :op2 "Roque" :op3 "Espinoza" :op4 "Leon"))
      :ARG1 (a / ask-01 :polarity -
            :ARG0 (p2 / person
                  :mod (c / country :wiki "Venezuela"
                        :name (n2 / name :op1 "Venezuela")))
            :ARG1 (t / thing
                  :ARG1-of (d / do-02
                        :ARG0 (p3 / person
                              :ARG0-of (h / have-org-role-91
