(a / and
      :op1 (i / involve-01
            :ARG1 (c / center
                  :mod (s / space))
            :ARG2 (l / launch-01
                  :ARG1 (m / missile
                        :mod (b / ballistics)
                        :mod (i2 / intercontinental)))
            :mod (a2 / also))
      :op2 (w / work-01
            :ARG0 c
            :ARG1 (p / plan)
            :purpose (d / develop-02
                  :ARG0 c
                  :ARG1 (s2 / system
                        :ARG0-of (d2 / defend-01)
                        :ARG0-of (w2 / warn-01
                              :time (e / early))))))

