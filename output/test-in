(l3 / law :polarity -
      :domain (p / pay-01
            :ARG1 (a / amount
                  :quant (c / certain))
            :manner (c2 / coin
                  :quant (l / load))
            :example (p2 / possible-01
                  :ARG1 (p3 / pay-01
                        :ARG0 (y / you)
                        :ARG1 (s / something
                              :example (m / monetary-quantity :quant 30
                                    :unit (p4 / pence)))
                        :manner (m2 / monetary-quantity :quant 1
                              :unit (p5 / pence))
                        :mod (o / only)))))

