(c / condemn-01
      :ARG0 (a / and
            :op1 (c2 / country :wiki "United_States"
                  :name (n / name :op1 "U.S."))
            :op2 (n2 / nation
                  :mod (o / other)
                  :location (w / world-region :wiki "Western_world"
                        :name (n3 / name :op1 "West")))
            :op3 (m / military :wiki "NATO"
                  :name (n4 / name :op1 "NATO")))
      :ARG1 (v / violence))

