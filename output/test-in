      :ARG0 (g / government-organization :wiki "Department_of_International_Relations_and_Cooperation"
            :name (n / name :op1 "Foreign" :op2 "Affairs" :op3 "Department")
            :mod (c4 / country :wiki "South_Africa"
                  :name (n5 / name :op1 "South" :op2 "Africa")))
      :ARG1 (c / country :wiki "Brazil"
            :name (n2 / name :op1 "Brazil")
            :ARG1-of (p / partner-01
                  :ARG0 c4
                  :ARG0-of (t / trade-01
                        :ARG2 c4)
                  :mod (l / large
                        :degree (m / most))
                  :location (w / world-region :wiki "Latin_America"
                        :name (n4 / name :op1 "Latin" :op2 "America")))))

