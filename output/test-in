(s / say-01
      :ARG0 (i2 / i)
      :ARG1 (i / instance
            :quant (m2 / many
                  :degree (t / too))
            :topic (c2 / crime-02
                  :ARG1 (h / heinous)
                  :ARG1-of (c3 / commit-02
                        :ARG0 (c4 / country :wiki "Japan" :name (n / name :op1 "Japan"))
                        :location (c5 / country :wiki "China" :name (n2 / name :op1 "China"))))
            :ARG1-of (r / record-01
                  :manner (d / difficult)
                  :manner (e / exhaust-01 :polarity -
                        :ARG1 (s2 / strip
                              :consist-of (b / bamboo)
                              :quant (a / all))))
            :ARG1-of (c6 / cite-01))
      :ARG2 (c / citizen
            :mod (f / fellow)
            :poss i2))

