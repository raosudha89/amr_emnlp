(s / state-01
      :ARG0 (p / person :wiki "Tang_Jiaxuan" :name (n / name :op1 "Tang"))
      :ARG1 (w / watch-01
            :ARG0 (c / community
                  :mod (i / international))
            :ARG1 o3
            :manner (c2 / close)
            :ARG1-of (c3 / cause-01
                  :ARG0 (w2 / will-02
                        :ARG0 (a2 / and
                              :op1 (c4 / country
                                    :quant (m / more))
                              :op2 (o2 / organization
                                    :mod (i2 / international)))
                        :ARG1 (c5 / conduct-01
                              :ARG0 a2
                              :ARG1 (a / and
                                    :op1 (d / dialogue-01
                                          :ARG0 a2
                                          :ARG2 o3)
                                    :op2 (c6 / cooperate-01
                                          :ARG0 a2
                                          :ARG1 (o3 / organization :wiki "Shanghai_Cooperation_Organisation" :name (n2 / name :op1 "SCO")))))))))

