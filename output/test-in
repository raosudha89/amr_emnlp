                  :ARG1 (o / organization :wiki "United_Nations_Office_on_Drugs_and_Crime" :name (n2 / name :op1 "U.N." :op2 "Drug" :op3 "Office"))))
      :ARG1 (p3 / produce-01
            :ARG0 (c / country :wiki "Afghanistan" :name (n3 / name :op1 "Afghanistan"))
            :ARG1 (o2 / opium
                  :quant (m / much
                        :mod (s2 / so)))
            :time (y / year
                  :mod (r / recent))
            :ARG0-of (c2 / cause-01
                  :ARG1 (a / and
                        :op1 (c3 / cut-back-05
                              :ARG0 (c4 / criminal-organization :wiki "Taliban" :name (n4 / name :op1 "Taliban"))
                              :ARG1 (c5 / cultivate-01
                                    :ARG1 (p4 / poppy)))
                        :op2 (s3 / stockpile-01
                              :ARG0 c4
