(s / support-01
      :ARG0 (p / presence
            :poss (t / troop
                  :mod (c / country :wiki "Russia"
                        :name (n / name :op1 "Russia"))))
      :ARG1 (c2 / conflict-01
            :ARG0 (a2 / area
                  :mod (s2 / separatist))
            :ARG1 (c3 / country :wiki "Georgia_(country)"
                  :name (n2 / name :op1 "Georgia"))
            :manner (b / bitter)))

