                  :op2 (c3 / city :wiki "Hong_Kong"
                        :name (n3 / name :op1 "Hong" :op2 "Kong"))
                  :op3 (c4 / country :wiki "Australia"
                        :name (n4 / name :op1 "Australia"))
                  :op4 (c5 / country :wiki "Indonesia"
