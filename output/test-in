(m / manage-02
      :ARG0 (g / government-organization
            :ARG0-of (a / administrate-01)
            :mod (p / person :wiki "George_W._Bush" :name (n / name :op1 "George" :op2 "W." :op3 "Bush")
                  :ARG0-of (h / have-org-role-91
                        :ARG1 (c6 / country :wiki "United_States" :name (n7 / name :op1 "United" :op2 "States"))
                        :ARG2 (p9 / president))))
      :ARG1 (s / synchronize-01
            :ARG0 a
            :ARG1 (p3 / policy
                  :mod c6)
            :ARG2 (a2 / and
                  :op1 (p4 / person
                        :ARG0-of (l2 / lead-02
                              :ARG1 (o / organization :wiki "European_Union" :name (n2 / name :op1 "European" :op2 "Union"))))
                  :op2 (p5 / person
                        :ARG0-of (l3 / lead-02
                              :ARG1 (c3 / country :wiki "United_Kingdom" :name (n3 / name :op1 "Great" :op2 "Britain"))))
                  :op3 (p6 / person
                        :ARG0-of (l4 / lead-02
                              :ARG1 (c4 / country :wiki "France" :name (n4 / name :op1 "France"))))
                  :op4 (p7 / person
                        :ARG0-of (l5 / lead-02
                              :ARG1 (c5 / country :wiki "Germany" :name (n5 / name :op1 "Germany")))))
            :purpose (p8 / press-01
                  :ARG0 a
                  :ARG2 (a3 / and
                        :op1 (s2 / sanction-02
                              :ARG1 (c2 / country :wiki "Iran" :name (n6 / name :op1 "Iran"))
                              :ARG1-of (t / tough-02
                                    :degree (m3 / more)))
                        :op2 (a4 / act-02
                              :mod (d / diplomacy)))))
      :time (m2 / month
            :mod (l / last)
            :time-of (i / in-power
                  :domain g)))

