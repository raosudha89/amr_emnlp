(c / characterize-01
      :ARG0 (p2 / person
            :ARG0-of (h / have-org-role-91
                  :ARG1 (c2 / country :wiki "France"
                        :name (n / name :op1 "France"))
                  :ARG2 (m / minister
                        :mod (f / foreign))))
      :ARG1 (r / raid-01
            :ARG1 (o / organization :wiki "People's_Mujahedin_of_Iran"
                  :name (n2 / name :op1 "People's" :op2 "Mujahedeen")
                  :location (c3 / city :wiki "Paris"
                        :name (n3 / name :op1 "Paris")))
            :time (r2 / recency))
      :ARG2 (p / part
            :part-of (f2 / fight-off-02
                  :ARG1 (t / terrorism))))

