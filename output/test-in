(a / and
      :op1 (e / establish-01
            :ARG1 (f / fund
                  :ARG1-of (g / guarantee-01)))
      :op2 (i / improve-01
            :ARG1 (a2 / and
                  :op1 (e2 / environment
                        :mod (w / work-01))
                  :op2 (m / mechanism
                        :mod (w3 / work-01))))
      :op3 (r / recruit-01
            :ARG1 (p / professional
                  :quant (m2 / multiple :op1 100000)
                  :ARG0-of (e3 / emigrate-01
                        :ARG2 (a3 / and
                              :op1 (c / country :wiki "United_States"
                                    :name (n / name :op1 "United" :op2 "States"))
                              :op2 (a4 / around
                                    :op1 (w2 / world))))
                  :mod (i2 / IT)
                  :ARG1-of (l / linger-01
                        :ARG2 a3))
            :direction (b / back)
            :destination (c2 / country :wiki "China"
                  :name (n2 / name :op1 "China")))
      :li (x / 1))

