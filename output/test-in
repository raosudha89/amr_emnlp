(s / state-01
      :ARG0 (p / person :wiki "Tang_Jiaxuan" :name (n / name :op1 "Tang"))
      :ARG1 (d / dominate-01
            :ARG0 (a / and
                  :op1 (p2 / peace)
                  :op2 (d2 / develop-01))
            :ARG1 (a2 / age
                  :time (c / current))
            :ARG1-of (c2 / cause-01
                  :ARG0 (a3 / and
                        :op1 (s2 / situation
                              :ARG1-of (g / general-02)
                              :location (w / world)
                              :ARG1-of (s3 / stable-03))
                        :op2 (d3 / develop-02
                              :ARG1 (e / economy)
                              :direction (f / forward)
                              :manner (w2 / wave))))
            :mod (s4 / still)))

