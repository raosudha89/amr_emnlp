      :ARG0 (i / i)
      :ARG1 (b / become-01
            :ARG1 (i2 / it)
            :ARG2 (p / problem
                  :mod (r / real)))
      :time (s / start-01
            :ARG1 (i3 / interrupt-01
                  :ARG0 (o / obsess-01)
