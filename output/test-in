(r / report-01
      :ARG0 (a / agency
            :mod (n / news)
            :mod (c / country :wiki "Iran" :name (n2 / name :op1 "Iran")))
      :ARG1 (s / state-01
            :ARG0 (p / person :wiki "Mahmoud_Ahmadinejad" :name (n3 / name :op1 "Ahmadinejad"))
            :ARG1 (h / have-03
                  :ARG0 c
                  :ARG1 (c2 / centrifuge :quant 6000
                        :ARG1-of (w / work-09))))
      :concession-of (r2 / remove-01
            :ARG0 a
            :ARG1 s
            :ARG2 (i / internet)
            :time (s2 / subsequent)))

