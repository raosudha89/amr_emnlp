# ::node	0.0.0.1.0	"U.S."	1-2
# ::node	0.0.1	nation	5-6
# ::node	0.0.1.0	other	3-4
# ::node	0.0.1.1	world-region	4-5
# ::node	0.0.1.1.0	"Western_world"	
# ::node	0.0.1.1.1	name	4-5
# ::node	0.0.1.1.1.0	"West"	4-5
# ::node	0.0.2	military	7-8
# ::node	0.0.2.0	"NATO"	
# ::node	0.0.2.1	name	7-8
# ::node	0.0.2.1.0	"NATO"	7-8
# ::node	0.1	cease-01	10-11
# ::node	0.1.0	fire-01	12-13
# ::root	0	demand-01
# ::edge	and	op1	country	0.0	0.0.0	
# ::edge	and	op2	nation	0.0	0.0.1	
# ::edge	and	op3	military	0.0	0.0.2	
# ::edge	cease-01	ARG1	fire-01	0.1	0.1.0	
# ::edge	country	name	name	0.0.0	0.0.0.1	
# ::edge	country	wiki	"United_States"	0.0.0	0.0.0.0	
# ::edge	demand-01	ARG0	and	0	0.0	
# ::edge	demand-01	ARG1	cease-01	0	0.1	
# ::edge	military	name	name	0.0.2	0.0.2.1	
# ::edge	military	wiki	"NATO"	0.0.2	0.0.2.0	
# ::edge	name	op1	"NATO"	0.0.2.1	0.0.2.1.0	
# ::edge	name	op1	"U.S."	0.0.0.1	0.0.0.1.0	
# ::edge	name	op1	"West"	0.0.1.1.1	0.0.1.1.1.0	
# ::edge	nation	location	world-region	0.0.1	0.0.1.1	
# ::edge	nation	mod	other	0.0.1	0.0.1.0	
# ::edge	world-region	name	name	0.0.1.1	0.0.1.1.1	
# ::edge	world-region	wiki	"Western_world"	0.0.1.1	0.0.1.1.0	
(d / demand-01
      :ARG0 (a / and
            :op1 (c / country :wiki "United_States"
                  :name (n / name :op1 "U.S."))
            :op2 (n2 / nation
                  :mod (o / other)
                  :location (w / world-region :wiki "Western_world"
                        :name (n3 / name :op1 "West")))
            :op3 (m / military :wiki "NATO"
                  :name (n4 / name :op1 "NATO")))
      :ARG1 (c2 / cease-01
            :ARG1 (f / fire-01)))

