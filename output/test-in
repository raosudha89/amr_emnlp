(p / possible-01
  :ARG1 (p2 / produce-01
            :ARG0 (c / centrifuge
                    :quant 5000)
            :ARG1 (m / material
                    :ARG0-of (s / suffice-01
                               :ARG1 (o / or
                                       :op1 (p3 / plant
                                              :mod (p4 / power))
                                       :op2 (b / bomb
                                              :mod (a / atom))))
                    :mod (n / nucleus)))
  :condition (e / enrich-01
               :ARG1 m
               :degree (h / high)))

