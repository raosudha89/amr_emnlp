(g2 / get-02 :mode interrogative
      :ARG0 (a2 / anyone)
      :ARG1 (e2 / experience-01
            :ARG0 a2
            :ARG1 (o / or
                  :op1 (h / handle-01
                        :ARG0 a2
                        :ARG1 (t / thing
                              :mod (s / sort
                                    :mod (t2 / this))))
                  :op2 (t3 / thing
                        :manner-of (g3 / go-06
                              :ARG1 (g4 / gain-02
                                    :ARG1 (r / respect-01
                                          :mod (s2 / some))
                                    :mod (a3 / again)))))
            :mod (a4 / any)))

