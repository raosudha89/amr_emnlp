(w / wish-01
      :ARG0 (i / i)
      :ARG1 (b / bear-02
            :ARG1 i
            :time (e / event :wiki "Long_March" :name (n2 / name :op1 "Long" :op2 "March")
                  :mod (p2 / passionate)
                  :ARG1-of (u / upsurge-00)
                  :mod (t3 / that)
                  :poss (p / person :wiki "Mao_Zedong" :name (n / name :op1 "Mao" :op2 "Zedong")))
            :purpose (a / and
                  :op1 (l / lose-02
                        :ARG0 i
                        :ARG1 (h / head
                              :part-of i))
                  :op2 (s / shed-00
                        :ARG0 i
                        :ARG1 (b2 / blood
                              :part-of i))
                  :op3 (e2 / extinguish-01
                        :ARG0 i
                        :ARG1 (p4 / person
                              :quant (a3 / all)
                              :ARG0-of (w2 / wrong-01
                                    :ARG1 (c2 / country :wiki "China" :name (n3 / name :op1 "China")))
                              :ARG0-of (h2 / have-rel-role-91
                                    :ARG1 c2
                                    :ARG2 (e3 / enemy)))
                        :ARG1-of (c / complete-02))
                  :op4 (s2 / spread-03
                        :ARG0 i
                        :ARG1 (p3 / prestige
                              :mod (d / dragon)
                              :poss (w3 / we)))))
      :degree (m / much
            :degree (v / very)))

