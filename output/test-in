      :ARG1 (a2 / and
            :op1 (m3 / map
                  :mod (c / city :wiki "Paris"
                        :name (n / name :op1 "Paris"))
                  :consist-of (s2 / street
                        :ARG1-of (r / relate-01
                              :ARG2 (e3 / embassy
                                    :poss (c5 / country :wiki "United_States"
                                          :name (n2 / name :op1 "United" :op2 "States"))))
                        :ARG1-of (m5 / mark-02
                              :manner (o / out))))
            :op2 (j / jacket
                  :mod (b2 / bulletproof))
            :op3 (e / equipment
                  :instrument-of (t2 / transmit-01))
            :op4 (c2 / computer
                  :quant (m / more-than :op1 200))
            :op5 (a / about
                  :op1 (m4 / monetary-quantity :quant 9000000
                        :unit (d / dollar
                              :mod (c4 / cash)
                              :mod c5))))
      :time (r2 / raid-01
            :ARG0 (p2 / police)))

