                                    :unit (d4 / day))))
                  :op3 (p2 / protect-01
                        :ARG0 c2
                        :ARG1 (a2 / and
                              :op1 (p3 / person
                                    :ARG0-of (f / fish-01))
                              :op2 (s3 / sovereignty
                                    :poss (c3 / country
                                          :poss (w3 / we)))))))
      :snt2 (f2 / fight-01
            :ARG0 (w / we)
            :ARG2 (w2 / war
                  :mod (s4 / scale
                        :mod (s5 / small))
                  :instrument-of (d2 / defend-01
                        :ARG0 w
