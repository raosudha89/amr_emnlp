      :ARG1 (p / person :wiki "Jacques_Chirac" :name (n / name :op1 "Chirac"))
      :destination (c / city :wiki "Krasnoznamensk,_Moscow_Oblast" :name (n2 / name :op1 "Krasnoznamernsk"))
      :purpose (m / meet-03
            :ARG0 p
            :ARG1 (p2 / person :wiki "Vladimir_Putin" :name (n3 / name :op1 "Vladimir" :op2 "Putin")
                  :ARG0-of (h / have-org-role-91
