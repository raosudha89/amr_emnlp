(m / multi-sentence
      :snt1 (a / attitude
            :ARG1-of (b / bad-07
                  :mod (s / such))
            :domain (t / this)
            :mod (r / really)
            :mod (w / well))
      :snt2 (p / possible-01
            :ARG1 (r2 / respect-01
                  :ARG1 (y / you))
            :condition (c / contrast-01
                  :ARG1 (n / nice-01
                        :ARG1 y)
                  :ARG2 (n2 / nice-01 :polarity -
                        :ARG1 y
                        :extent (l / let-01
                              :ARG0 y
                              :ARG1 (d / do-02
                                    :ARG0 (t2 / they)
                                    :ARG1 (a2 / anything)
                                    :ARG2 y)
                              :mod (j / just))))
            :mod (o / of-course))
      :snt3 (c2 / contrast-01
            :ARG1 (n3 / nice-01 :polarity -
