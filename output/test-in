      :ARG0 (p / person
            :ARG0-of (h / have-org-role-91
                  :ARG1 (o / organization :wiki "United_Nations"
                        :name (n / name :op1 "United" :op2 "Nations"))
                  :ARG3 (i / inspect-01)))
      :ARG1 (s2 / site :quant 7
            :mod (c / country :wiki "Iraq"
                  :name (n2 / name :op1 "Iraq")))
      :ARG2 (w / weapon
            :ARG1-of (b / ban-01))
      :time (d / date-entity :month 12 :day 28 :year 2002))

