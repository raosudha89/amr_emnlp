                  :op1 (c2 / country :wiki "China" :name (n / name :op1 "China"))
                  :op2 (c3 / country :wiki "Kyrgyzstan" :name (n2 / name :op1 "Kyrgyzstan")))
            :ARG2 (a3 / and
                  :op1 (e / expand-01
                        :ARG0 a
                        :ARG1 (c4 / cooperate-01
                              :ARG0 a
                              :ARG1 (c6 / country
                                    :mod (o / other)
                                    :ARG0-of (h / have-org-role-91
                                          :ARG1 (o2 / organization :wiki "Shanghai_Cooperation_Organisation" :name (n3 / name :op1 "SCO"))
                                          :ARG2 (m / member)))
                              :ARG2 (a7 / and
                                    :op1 (e2 / economy)
                                    :op2 (f / field
                                          :mod (o5 / other)))))
                  :op2 (p / promote-02
                        :ARG0 a
                        :ARG1 (r2 / relation-03
                              :ARG0 o2
                              :ARG2 (a4 / and
                                    :op1 (c5 / country
                                          :mod (o3 / other))
                                    :op2 (o4 / organization
                                          :mod (i / international)))))
                  :op3 (s2 / strengthen-01
                        :ARG0 a
                        :ARG1 (r3 / role
                              :poss o2
                              :purpose (m2 / maintain-01
                                    :ARG1 (a5 / and
                                          :op1 (a8 / and
                                                :op1 (p3 / peace
