(r / raid-01
      :ARG0 (p / police
            :mod (c2 / country :wiki "France"
                  :name (n4 / name :op1 "France")))
      :ARG1 (h / headquarters
            :poss (o / organization :wiki "People's_Mujahedin_of_Iran"
                  :name (n / name :op1 "People's" :op2 "Mujahedeen"))
            :location (n3 / northwest
                  :part-of (c / city :wiki "Paris"
                        :name (n2 / name :op1 "Paris"))))
      :time (d / date-entity :month 6 :day 22 :year 2003))

