      :ARG0 (p2 / person :wiki -
            :name (n / name :op1 "Nikolai" :op2 "N." :op3 "Urakov")
            :ARG0-of (h / have-org-role-91
                  :ARG2 (g / general)))
      :ARG1 (c / contribute-01
            :ARG0 (a / and
                  :op1 (c2 / country :wiki "United_States"
                        :name (n2 / name :op1 "United" :op2 "States"))
                  :op2 (c3 / country
                        :location (w / world-region :wiki "Western_world"
                              :name (n3 / name :op1 "West")))
                  :ARG0-of (c4 / contribute-01
                        :ARG1 (a7 / about
                              :op1 (m / monetary-quantity :quant 6000000
                                    :unit (d / dollar
                                          :mod c2)))
                        :ARG2 (t / transform-01
                              :ARG1 (c5 / complex
                                    :mod (b3 / bioweapon)
                                    :time (f / former)))))
            :ARG1 (r2 / rate-entity-91
                  :ARG1 (m2 / monetary-quantity :quant 500000
                        :unit d
                        :mod (a6 / another))
                  :ARG2 (t2 / temporal-quantity :quant 1
                        :unit (y / year)))
            :purpose (p / pay-01
                  :ARG0 a
                  :ARG1 m2
                  :ARG3 (a4 / and
                        :op1 (b2 / bill
                              :mod (e / electric))
                        :op2 (a5 / arrear)
                        :poss (r / research-institute :wiki -
                              :name (n4 / name :op1 "State" :op2 "Scientific" :op3 "Center" :op4 "of" :op5 "Applied" :op6 "Microbiology"))))))

