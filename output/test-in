(m / multi-sentence
      :snt1 (a / and
            :op1 (s / shake-01
                  :ARG1 (p / piece
                        :part-of (l / land)
                        :ARG1-of (t / tremble-01
                              :location (r / relative-position
                                    :op1 (e / embrace-01
                                          :ARG0 (m3 / motherland)
                                          :ARG1 p)
                                    :quant (f / far)))
                        :mod (t2 / that)))
            :op2 (m2 / moan-01
                  :ARG0 p)
            :op3 (c / call-02
                  :ARG0 p)
            :time (n / now))
      :snt2 (s2 / say-01
            :ARG1 (c3 / carry-01 :mode imperative
                  :ARG0 (w2 / we)
                  :ARG1 (t3 / thing
                        :ARG1-of (w / wish-01
                              :ARG0 (p3 / person
                                    :ARG0-of (h / have-org-role-91
                                          :ARG2 (g / general)
                                          :time (p2 / previous)))
                              :ARG1-of (f5 / fulfill-01 :polarity -)))
                  :direction (f4 / forward)
                  :manner (m4 / move-01
                        :ARG1 w2
                        :direction (f3 / forward)
                        :manner (c4 / courageous)))
            :ARG2 (c2 / citizen
                  :mod (f2 / fellow))))

