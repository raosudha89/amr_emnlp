      :ARG1 (a / and
            :op1 (u2 / university
                  :mod (m / mainland
                        :part-of (c3 / country :wiki "China" :name (n2 / name :op1 "China"))))
            :op2 (b / business
                  :mod m)
            :op3 (s / sector
                  :mod (g / government-organization
                        :ARG0-of (g2 / govern-01))
                  :mod m))
      :ARG2 (c4 / coordinate-01
            :ARG0 u
            :ARG1 (a2 / and
                  :op1 (p / program
                        :mod (t / train-01))
                  :op2 (c5 / center
                        :mod (r / research-01)))
