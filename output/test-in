                        :ARG1 (r / reactor
                              :purpose (r2 / research-01)
                              :mod (w / water
                                    :mod (h / heavy))
                              :mod (c / country :wiki "Iran" :name (n2 / name :op1 "Iran"))
                              :ARG1-of (n3 / near-02
                                    :ARG2 (c2 / city :wiki "Arak,_Iran" :name (n4 / name :op1 "Arak"))))
                        :ARG1-of (s4 / significant-02)))
            :op2 (p3 / progress-01
                  :ARG1 r
                  :ARG3 (f / frame
                        :mod (b / building))
                  :ARG4 (f2 / facility
                        :ARG1-of (f3 / finish-01
                              :degree (l / large)))
                  :duration (b2 / before
                        :op1 (n5 / now)
                        :duration (t / temporal-quantity :quant 1.5
                              :unit (y / year))))))

