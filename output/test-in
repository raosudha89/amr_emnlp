(g / greet-01
      :ARG0 (a / and
            :op1 (p / person :wiki "Vladimir_Putin"
                  :name (n / name :op1 "Putin"))
            :op2 (p2 / person :wiki "Sergei_Ivanov"
                  :name (n2 / name :op1 "Sergei" :op2 "Ivanov")
                  :ARG0-of (h / have-org-role-91
                        :ARG1 (c / country :wiki "Russia"
                              :name (n3 / name :op1 "Russia"))
                        :ARG2 (m / minister
                              :mod (d2 / defense)))))
      :ARG1 (p3 / person :wiki "Jacques_Chirac"
            :name (n4 / name :op1 "Chirac"))
      :location (c2 / center
            :mod (s2 / space)
            :location (c4 / city :wiki "Krasnoznamensk,_Moscow_Oblast"
                  :name (n6 / name :op1 "Krasnoznamernsk")
                  :location (r / relative-position
                        :op1 (c3 / city :wiki "Moscow"
                              :name (n5 / name :op1 "Moscow"))
                        :quant (s3 / some
                              :op1 (d3 / distance-quantity :quant 40
                                    :unit (k2 / kilometer)))
                        :direction (s4 / southwest)))))

