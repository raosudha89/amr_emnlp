            :mod (c3 / complex)
            :ARG0-of (f / face-01
                  :ARG1 (t3 / team
                        :mod (p / policy
                              :mod (f2 / foreign))
                        :mod (i / incoming)
                        :poss (p3 / person :wiki "Barack_Obama" :name (n4 / name :op1 "Barack" :op2 "Obama")
