      :ARG0 (p / person :wiki "Dominique_de_Villepin"
            :name (n / name :op1 "De" :op2 "Villepin"))
      :ARG1 (a / act-02
            :ARG0 (p2 / person
                  :mod (c / country :wiki "France"
                        :name (n2 / name :op1 "France")))
            :ARG1-of (b / base-02
                  :ARG2 (j / justice)
                  :manner (s2 / strict))
            :manner (d / determine-01
                  :ARG0 p2
                  :ARG1 (t2 / try-01
                        :ARG0 p2
                        :ARG1 (f / fight-01
                              :ARG0 p2
                              :ARG1 (t / terrorism))
                        :mod (a3 / all)))))

