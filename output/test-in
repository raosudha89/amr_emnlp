                  :ARG1 (c / country :wiki "North_Korea" :name (n / name :op1 "North" :op2 "Korea"))
                  :ARG2 (o / official)))
      :ARG1 (p / proceed-01
            :ARG0 p2
            :ARG1 (a / agree-01
                  :ARG1 (b / begin-01
                        :ARG1 (d3 / decommission-01
                              :ARG1 (r3 / reactor :wiki "Yongbyon_Nuclear_Scientific_Research_Center" :name (n4 / name :op1 "Yongbyon"))))
                  :time (d / date-entity :month 2 :year 2007)))
      :time (u / until
            :op1 (t / transfer-01
                  :ARG1 (f2 / fund
                        :ARG1-of (v / value-01
                              :ARG2 (m / monetary-quantity :quant 25000000
                                    :unit (d2 / dollar)))
                        :location (a2 / account
                              :ARG1-of (l / link-01
                                    :ARG2 c))
                        :ARG1-of (h / hold-01
                              :location (b2 / bank
                                    :mod (c2 / city :wiki "Macau" :name (n3 / name :op1 "Macao")))))
                  :ARG2 c)))

