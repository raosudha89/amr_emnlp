(s / state-01
      :ARG0 (p6 / person
            :ARG0-of (h6 / have-org-role-91
                  :ARG2 (d2 / diplomat)))
      :ARG1 (r / reach-01 :polarity -
            :ARG0 (a2 / and
                  :op1 (p2 / person
                        :ARG0-of (h2 / have-org-role-91
                              :ARG1 (c / country :wiki "United_States" :name (n / name :op1 "United" :op2 "States"))
                              :ARG2 (o / official)))
                  :op2 (p3 / person
                        :ARG0-of (h3 / have-org-role-91
                              :ARG1 (c2 / continent :wiki "Europe" :name (n2 / name :op1 "Europe"))
                              :ARG2 (o2 / official)))
                  :op3 (p4 / person
                        :ARG0-of (h4 / have-org-role-91
                              :ARG1 (c3 / country :wiki "Russia" :name (n3 / name :op1 "Russia"))
                              :ARG2 (o3 / official)))
                  :op4 (p5 / person
                        :ARG0-of (h5 / have-org-role-91
                              :ARG1 (c4 / country :wiki "China" :name (n4 / name :op1 "China"))
                              :ARG2 (o4 / official))))
            :ARG1 (a / agree-01
                  :ARG0 a2
                  :ARG1 (a3 / act-02
                        :ARG0 a2
                        :ARG1 (p / persuade-01
                              :ARG0 a2
                              :ARG1 (c6 / country :wiki "Iran" :name (n6 / name :op1 "Iran"))
                              :ARG2 (h / halt-01
                                    :ARG0 c6
                                    :ARG1 (e / enrich-01
                                          :ARG1 (u / uranium)
                                          :location (f2 / facility
                                                :mod c6
                                                :location (c7 / city :wiki "Natanz" :name (n7 / name :op1 "Natanz"))))))
                        :degree (f / further)))
            :time (m2 / meet-03
                  :time (d3 / date-entity :year 2008 :month 11 :day 13)
                  :location (c5 / city :wiki "Paris" :name (n5 / name :op1 "Paris"))))
      :ARG2 (m / media)
      :time (d / date-entity :year 2008 :month 11 :day 14))

