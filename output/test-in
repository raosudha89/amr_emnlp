      :ARG0 (a / and
            :op1 (p / person :wiki "Javier_Solana"
                  :name (n / name :op1 "Solana"))
            :op2 (p2 / person :wiki "William_J._Burns"
                  :name (n2 / name :op1 "Burns"))
            :op3 (e / envoy
                  :mod (c / continent :wiki "Europe"
                        :name (n3 / name :op1 "Europe"))))
      :ARG1 (d / deadline
            :prep-for (r / respond-01
                  :ARG0 (c2 / country :wiki "Iran"
                        :name (n4 / name :op1 "Iran"))
                  :ARG2 (p3 / positive)))
      :time (b / before
            :op1 (i / initiate-01
                  :ARG0 a
                  :ARG1 (s2 / sanction-02
                        :mod (n5 / new)
                        :ARG2-of (i2 / include-01
                              :ARG1 (p4 / prohibit-01
                                    :ARG1 (s3 / sell-01
                                          :ARG1 (p5 / product
