                  :op2 (p3 / police
                        :location (r / region
                              :mod (o2 / other))))
            :ARG1-of (a2 / activity-06
                  :ARG0 p))
      :ARG2 (e / effort
            :poss (p / police
                  :mod (c / city :wiki "Hong_Kong" :name (n / name :op1 "Hong" :op2 "Kong")))
