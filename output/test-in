(s / state-01
      :ARG0 (g / government-organization :wiki "Department_of_International_Relations_and_Cooperation"
            :name (n / name :op1 "Foreign" :op2 "Affairs" :op3 "Department")
            :mod (c5 / country :wiki "South_Africa"
                  :name (n6 / name :op1 "South" :op2 "Africa")))
      :ARG1 (o / outline-01
            :ARG0 (a / agree-01
                  :ARG0 (a2 / and
                        :op1 (c / country :wiki "India"
                              :name (n2 / name :op1 "India"))
                        :op2 (c2 / country :wiki "Brazil"
                              :name (n3 / name :op1 "Brazil"))
                        :op3 c5
                        :mod (h / historic)
                        :mod (s2 / strategy))
                  :ARG2-of (r / result-01
                        :ARG1 (o2 / organization :wiki -
                              :name (n5 / name :op1 "Brazil-Africa" :op2 "Forum"))))
            :ARG1 (p / plan-01
                  :ARG0 a
                  :purpose (c4 / cooperate-01
                        :ARG0 a2
                        :ARG2 (a4 / and
                              :op1 (p2 / politics)
                              :op2 (e / economy))
                        :mod (t / trilateral)))))

