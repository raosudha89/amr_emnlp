      :ARG0 (a / and
            :op1 (c / country :wiki "United_States" :name (n / name :op1 "US"))
            :op2 (c2 / country :wiki "Israel" :name (n2 / name :op1 "Israel"))
            :op3 (c3 / continent :wiki "Europe" :name (n3 / name :op1 "Europe"))
            :op4 (p / person
                  :ARG1-of (e / expert-01
                        :ARG2 (c4 / control-01
                              :ARG1 (a2 / arm)))
                  :mod (w / world-region :wiki "Western_world" :name (n4 / name :op1 "West"))
                  :quant (m / majority)))
      :ARG1 (p2 / pursue-01
            :ARG0 (c5 / country :wiki "Iran" :name (n5 / name :op1 "Iran"))
            :ARG1 (c6 / capable-01
                  :ARG1 c5
                  :ARG2 (p3 / produce-01
                        :ARG0 c5
                        :ARG1 (b2 / bomb)
                        :condition (d / decide-01)
                        :time (i / immediate)))))

