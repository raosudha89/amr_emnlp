(a / and
      :op1 (s / support-01
            :ARG0 (g / government-organization
                  :ARG0-of (g2 / govern-01
                        :ARG1 (c / country :wiki "Germany" :name (n / name :op1 "Germany"))))
            :ARG1 (b / bid-03
                  :ARG0 (g3 / government-organization
                        :ARG0-of (g4 / govern-01
                              :ARG1 (c2 / country :wiki "Vietnam" :name (n2 / name :op1 "Vietnam"))))
                  :ARG1 (m / membership
                        :mod (p / permanence :polarity -)
                        :mod (o / organization :wiki "United_Nations_Security_Council" :name (n3 / name :op1 "Security" :op2 "Council")
                              :part-of (o2 / organization :wiki "United_Nations" :name (n4 / name :op1 "UN")))
                        :time (t / term
                              :mod (d / date-interval
                                    :op1 (d2 / date-entity :year 2008)
                                    :op2 (d3 / date-entity :year 2009))))))
      :op2 (e / encourage-01
            :ARG0 g
            :ARG1 (a2 / and
                  :op1 (e2 / enterprise
                        :ARG1-of (s2 / size-01
                              :ARG2 (s3 / small))
                        :mod c)
                  :op2 (e3 / enterprise
                        :ARG1-of (s4 / size-01
                              :ARG2 (m2 / medium))
                        :mod c))
            :ARG2 (d4 / do-02
                  :ARG0 a2
                  :ARG1 (b2 / business)
                  :ARG4 c2)))

