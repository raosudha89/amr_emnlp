      :domain (w / world-region :wiki "Caucasus"
            :name (n / name :op1 "Caucasus"))
      :mod (i / importance)
      :purpose (f / flow-01
            :ARG1 (o / oil)
            :source (s / sea :wiki "Caspian_Sea"
